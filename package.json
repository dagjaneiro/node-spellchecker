    "prebuild-ia32": "prebuild --all -a ia32 --strip",
    "upload": "node ./upload.js"
  },
  "devDependencies": {
    "jasmine-focused": "1.x",
    "download-github-release": "^0.3.1",
    "prebuild": "^9.0.0"
  },
  "dependencies": {
    "prebuild-install": "^5.3.0",
    "any-promise": "^1.3.0",
    "nan": "^2.10.0"
  }
